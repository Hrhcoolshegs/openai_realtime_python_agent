# Environment Variables for Python WebSocket Server
# Copy this file to .env and fill in your actual values

# OpenAI API Key (required)
OPENAI_API_KEY="your_openai_api_key_here"

# Public URL for ngrok tunnel (required for Twilio webhook)
PUBLIC_URL="https://your-ngrok-url.ngrok-free.app"

# Server Configuration
PORT=8081
HOST=0.0.0.0

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional: Twilio credentials (if needed for additional features)
TWILIO_ACCOUNT_SID=""
TWILIO_AUTH_TOKEN=""

# Supabase Database Configuration (required for Eev AI features)
SUPABASE_URL="https://your-project-id.supabase.co"
SUPABASE_KEY="your_supabase_anon_key_here"
